! general settings
?.saveline:        1500
?.termName:        xterm-256color
?.urgentOnBell:    false
?.visualBell:      false
?.iso14755:        false
?.iso14755_52:     false
?.meta8:           false
?.metaSendsEscape: true
?.iconfile:        /usr/share/icons/breeze-dark/apps/64/utilities-terminal.svg

! appearance
?.depth:             32
?.transparent:       true
?.shading:           10
?.scrollBar:         false
?.scrollTtyOutput:   false
?.scrollWithBuffer:  true
?.scrollTtyKeypress: true

! Dracula color scheme
?.foreground: #F8F8F2
